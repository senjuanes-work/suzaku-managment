{
  "name": "Suzaku Management",
  "description": "Suzaku Management",
  "version": "1.0.2",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "host_permissions" : [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/16.png",
      "32": "/icons/32.png",
      "48": "/icons/48.png",
      "128": "/icons/128.png"
    }
  },
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  }
}